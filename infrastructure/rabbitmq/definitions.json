{
  "rabbit_version": "3.12",
  "users": [
    {
      "name": "datban",
      "password_hash": "YOUR_HASHED_PASSWORD",
      "hashing_algorithm": "rabbit_password_hashing_sha256",
      "tags": "administrator"
    }
  ],
  "vhosts": [
    {
      "name": "/"
    }
  ],
  "permissions": [
    {
      "user": "datban",
      "vhost": "/",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    }
  ],
  "parameters": {},
  "global_parameters": {
    "basic": {
      "credit_write_buffer": 131072,
      "credit_write_adapter": "simple"
    }
  },
  "policies": [
    {
      "vhost": "/",
      "name": "ha-all",
      "pattern": ".*",
      "apply-to": "all",
      "definition": {
        "ha-mode": "all",
        "ha-sync-mode": "automatic"
      },
      "priority": 0,
      "components": {
        "exchange": true,
        "queue": true
      },
      "resource-timeout": 0
    }
  ],
  "queues": [
    {
      "name": "user.reservations",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-queue-type": "classic"
      }
    },
    {
      "name": "restaurant.reservations",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-queue-type": "classic"
      }
    },
    {
      "name": "reservation.payments",
